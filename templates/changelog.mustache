{{!
    Changelog view.

    Context
    * changes
}}
<div data-region="admin-announcement-changelog">
<table class="generaltable fullwidth">
    <thead>
        <tr>
            <th scope="col">{{#str}}changelogview_newvalue, block_admin_announcement{{/str}}</th>
            <th scope="col">{{#str}}changelogview_oldvalue, block_admin_announcement{{/str}}</th>
            <th scope="col">{{#str}}changelogview_timemodified, block_admin_announcement{{/str}}</th>
        </tr>
    </thead>
    <tbody>
        {{#changes}}
        <tr>
            <td>{{value}}</a></td>
            <td>{{oldvalue}}</a></td>
            <td>{{timemodified}}</td>
        </tr>
        {{/changes}}
    </tbody>
</table>
</div>
